{
  "name": "ai-music-player",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./build/server.js",
    "start-dev": "nodemon ./build/server.js",
    "build": "npm install typescript && npm run build-full",
    "build-full": "tsc --project tsconfig.frontend.json & tsc --project tsconfig.server.json",
    "build-frontend": "tsc --project tsconfig.frontend.json",
    "build-frontend-dev": "yarn run build-frontend --watch",
    "build-server": "tsc --project tsconfig.server.json",
    "build-server-dev": "yarn run build-server --watch"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/node-fetch": "^2.5.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
